<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pinterest</title>
</head>
<body>
    <% include ./partials/header.ejs %>
    <div class="w-full min=h-screen px-10 py-8 bg-zinc-800">
        <div class="cards flex flex-wrap gap-10 rounded-lg ">
            <div>
                <% user.posts.forEach(function(elem){ %>
                    <div class="card w-32 h-40 bg-sky-200 rounded-lg overflow-hidden">
                        <img class="w-full h-full object-cover" src="/images/uploads/<%=elem.image%>" alt="">
                    </div>
                    <h5 class="mt-2 text-sm"><%= elem.title%></h5>

                })%>
                

            </div>
            
        </div>
    </div>
</body>
</html>